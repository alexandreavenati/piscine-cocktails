{% extends 'base.html.twig' %}

{% block main %}
<main>

<section class="cocktail-list">
	{% for category in categories %}
		<article class="cocktail-card">
			<div class="cocktail-info">
				<h2>{{ category.nom }}</h2>
				<p>{{ category.description }}</p>
			</div>
		</article>
	{% endfor %}
</section>

</main>
{% endblock main %}